{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "react/jsx-props-no-spreading":0,
        "consistent-return": 0,
        "react/prop-types": 0,
        "jsx-a11y/control-has-associated-label": 0,
        "react/no-array-index-key": 0
    }
}
